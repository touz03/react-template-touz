{
  "name": "react-template-touz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "build": "tsc --noEmit && swc src -d dist && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "lint": "biome lint ./src",
    "check": "biome check --apply ./src",
    "check:ci": "biome ci ./src",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.63.0",
    "jotai": "^2.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1"
  }
}
